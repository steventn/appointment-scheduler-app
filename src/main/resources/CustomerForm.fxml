        <Label fx:id="firstLevelDivisionLabel" layoutX="66.0" layoutY="322.0" text="First Level-Division" />
        <Label fx:id="countryLabel" layoutX="66.0" layoutY="369.0" text="Country" />
        <TextField fx:id="customerIDField" editable="false" layoutX="180.0" layoutY="133.0" promptText="Auto Gen - Disabled" />
        <TextField fx:id="customerNameField" layoutX="180.0" layoutY="180.0" />
        <TextField fx:id="customerAddressField" layoutX="180.0" layoutY="224.0" />
