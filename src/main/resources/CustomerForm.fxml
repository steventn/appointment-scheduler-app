            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="customerPhoneNumberLabel" layoutX="66.0" layoutY="413.0" text="Phone Number" />
      <TextField fx:id="customerPhoneNumberField" layoutX="180.0" layoutY="408.0" />
